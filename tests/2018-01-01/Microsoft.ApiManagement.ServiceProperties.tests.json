        {
          "message": "No enum match for: \"2015-10-12\"",
          "dataPath": "/apiVersion",
          "schemaPath": "/properties/apiVersion/type"
        }
      ],
      "definition": "https://raw.githubusercontent.com/t-anba/azure-resource-manager-schemas/t-anbaAzSDeploymentTemplate/schemas/2018-01-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_properties",
      "json": {
        "type": "Microsoft.ApiManagement/invalidType",
        "apiVersion": "2015-10-12",
        "name": "testprop2",
        "properties": {
          "displayName": "prop3name",
          "value": "propValue",
          "tags": [
            "foo",
            "bar"
          ],
          "secret": true
        }
      }
    }
  ]
