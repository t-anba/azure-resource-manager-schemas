{
  "tests": [
    {
      "name": "ContainerServices - Basic",
      "definition": "https://raw.githubusercontent.com/t-anba/azure-resource-manager-schemas/t-anbaAzSDeploymentTemplate/schemas/2017-07-01/Microsoft.ContainerService.json#/resourceDefinitions/containerServices",
      "json": {
        "name": "[parameters('resourceName')]",
        "type": "Microsoft.ContainerService/containerServices",
        "apiVersion": "2017-07-01",
        "location": "[resourceGroup().location]",
        "properties": {
          "orchestratorProfile": {
            "orchestratorType": "[parameters('orchestratorType')]"
          },
          "masterProfile": {
            "dnsPrefix": "[parameters('dnsPrefix')]",
            "vmSize": "[parameters('masterVmSize')]"
          },
          "linuxProfile": {
            "adminUsername": "[parameters('adminUsername')]",
            "ssh": {
              "publicKeys": [
                {
                  "keyData": "[parameters('keyData')]"
                }
              ]
            }
          }
        }
      }
    }
  ]
}
