{
  "tests": [
    {
      "name": "ApiManagement Service Identity Providers - Minimal",
      "definition": "https://raw.githubusercontent.com/t-anba/azure-resource-manager-schemas/t-anbaAzSDeploymentTemplate/schemas/2018-01-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_identityProviders",
      "json": {
        "apiVersion": "2018-01-01",
        "type": "Microsoft.ApiManagement/service/identityProviders",
        "name": "facebook",
        "properties": {
          "clientId": "facebookid",
          "clientSecret": "facebookapplicationsecret",
          "type": "facebook"
        }
      }
    },
    {
      "name": "ApiManagement Service Identity Providers - invalidType",
      "expectedErrors": [
        {
          "message": "No enum match for: \"Microsoft.ApiManagement/invalidType\"",
          "dataPath": "/type",
          "schemaPath": "/properties/type/type"
        },
        {
          "message": "No enum match for: \"2015-10-12\"",
          "dataPath": "/apiVersion",
          "schemaPath": "/properties/apiVersion/type"
        }
      ],
      "definition": "https://raw.githubusercontent.com/t-anba/azure-resource-manager-schemas/t-anbaAzSDeploymentTemplate/schemas/2018-01-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_identityProviders",
      "json": {
        "type": "Microsoft.ApiManagement/invalidType",
        "apiVersion": "2015-10-12",
        "name": "facebook",
        "properties": {
          "clientId": "facebookid",
          "clientSecret": "facebookapplicationsecret",
          "type": "facebook"
        }
      }
    }
  ]
}