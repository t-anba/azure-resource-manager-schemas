{
  "tests": [
    {
      "name": "ApiManagement Service Properties - Minimal",
      "definition": "https://raw.githubusercontent.com/t-anba/azure-resource-manager-schemas/t-anbaAzSDeploymentTemplate/schemas/2018-01-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_properties",
      "json": {
        "apiVersion": "2018-01-01",
        "type": "Microsoft.ApiManagement/service/properties",
        "name": "testprop2",
        "properties": {
          "displayName": "prop3name",
          "value": "propValue",
          "tags": [
            "foo",
            "bar"
          ],
          "secret": true
        }
      }
