{
  "tests": [
    {
      "name": "ApiManagement Service Diagnostic Operation - Minimal",
      "definition": "https://raw.githubusercontent.com/t-anba/azure-resource-manager-schemas/t-anbaAzSDeploymentTemplate/schemas/2017-03-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_diagnostics",
      "json": {
        "apiVersion": "2017-03-01",
        "type": "Microsoft.ApiManagement/service/diagnostics",
        "name": "default",
        "properties": {
          "enabled": true
        }
      }
    },
    {
      "name": "ApiManagement Service Diagnostic Operation - invalidType",
      "expectedErrors": [
        {
          "message": "No enum match for: \"Microsoft.ApiManagement/invalidType\"",
          "dataPath": "/type",
          "schemaPath": "/properties/type/type"
        },
        {
          "message": "No enum match for: \"2015-10-12\"",
          "dataPath": "/apiVersion",
          "schemaPath": "/properties/apiVersion/type"
        }
      ],
      "definition": "https://raw.githubusercontent.com/t-anba/azure-resource-manager-schemas/t-anbaAzSDeploymentTemplate/schemas/2017-03-01/Microsoft.ApiManagement.json#/resourceDefinitions/service_diagnostics",
      "json": {
        "type": "Microsoft.ApiManagement/invalidType",
        "apiVersion": "2015-10-12",
        "name": "default",
        "properties": {
          "enabled": true
        }
      }
    }
  ]
}