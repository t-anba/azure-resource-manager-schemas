{
    "tests": [
        {
            "name": "Registation test",
            "definition": "https://raw.githubusercontent.com/t-anba/azure-resource-manager-schemas/t-anbaAzSDeploymentTemplate/schemas/2017-06-01/Microsoft.AzureStack.json#/resourceDefinitions/registrations",
            "json": {
                "apiVersion": "2017-06-01",
                "type": "Microsoft.AzureStack/registrations",
                "name": "[variables('registrationName')]",
                "location": "global",
                "properties": {
                    "registrationToken": "[variables('registationToken')]"
                }
            }
        },
        {
            "name": "Register with customer subscriptions test",
            "definition": "https://raw.githubusercontent.com/t-anba/azure-resource-manager-schemas/t-anbaAzSDeploymentTemplate/schemas/2017-06-01/Microsoft.AzureStack.json#/resourceDefinitions/registrations",
            "json": {
                "apiVersion": "2017-06-01",
                "type": "Microsoft.AzureStack/registrations",
                "name": "[variables('registrationName')]",
                "location": "global",
                "properties": {
                    "registrationToken": "[variables('registationToken')]"
                }
            },
            "resources" : [
                {
                    "apiVersion": "2017-06-01",
                    "type": "Microsoft.AzureStack/registrations",
                    "name": "[variables('registrationName')]",
                    "location": "global",
                    "properties": {
                        "tenantId": "[variables('tenantId')]"
                    }
                }   
            ]
        }
    ]
}